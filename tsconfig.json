{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": ["dom", "es2015", "es2017"]
  },
  "include": [
    "./gatsby-*.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "node_modules/@types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules/",
    "dist"
  ]
}