{
  "compilerOptions": {
    "outDir": "./build/",
    "sourceMap": true,
    "removeComments": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "lib": [
      "dom",
      "es2015",
      "es2017"
    ]
  },
  "include": [
    "./gatsby-*.ts",
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules/**/*",
    "dist"
  ]
}